<h1>Pianos</h1>

<ul>
<% @pianos.each do |piano| %>
  <p>
  <%= link_to piano.address, piano_path(piano) %>
  </p>
<% end %>
</ul>

<p>
  <%= link_to "Create piano", new_piano_url %>
</p>

<p>
<%= link_to "Home", root_path %>
</p>


<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>
